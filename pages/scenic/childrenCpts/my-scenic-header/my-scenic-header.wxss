.scenic-header {
  position:  relative;
  width: 100%;
  overflow: hidden;
}

.scenic-header .image-box {
  width: 100%;

}

.scenic-header .image-box .image {
  background-size: cover;
  background-repeat: no-repeat;
  background-position: 50%;
  padding-bottom: 58%;
}

.scenic-header .title {
  position: relative;
}

.scenic-header .title .avatar-box {
  position: absolute;
  left: 50%;
  top: -64rpx;
  transform: translateX(-50%);
  width: 130rpx;
  height: 130rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  background-color: #5acbda;
  border-radius: 50%;
  border: 8rpx solid #fbf7ed;
}

.scenic-header .title .avatar-box.type-11 {
  background-color: #5acbda;
}

.scenic-header .title .avatar-box.type-15 {
  background-color: #bad424;
}

.scenic-header .title .avatar-box.type-19 {
  background-color: #bad424;
}

.scenic-header .title .avatar-box.type-6 {
  background-color: #fc5d7b;
}

.scenic-header .title .avatar-box.type-5 {
  background-color: #ffcb5b;
}

.scenic-header .title .avatar-box.type-10 {
  background-color: #a683e5;
}

.scenic-header .title .name {
  font-size: var(--font-size-large-x);
  text-align: center;
  color: #4a4a4a;
  padding-top: 70rpx;
}

.scenic-header .title .rating-box {
  text-align: center;
  margin-top: 20rpx;
  padding-bottom: 26rpx;
}

.scenic-header .title .desc {
  width: 78%;
  text-align: center;
  margin: 0 auto;
  padding-bottom: 66rpx;
}
