.schedule-item {
  width: 100%;
  margin-bottom: 30rpx;
}

.schedule-item .place {
  display: flex;
  width: 100%;
  height: 90rpx;
  line-height: 52rpx;
  padding: 18rpx 0;
  background-color: var(--color-theme);
  color: #fff;
  font-size: var(--font-size-medium-x);
  border-bottom: 6rpx solid #2e9dab;
}

.schedule-item .place view {
  display: inline-block;
  vertical-align: middle;
}

.schedule-item .place .title-box {
  flex: 1;
}

.schedule-item .place .icon-place {
  margin: 0 30rpx;
}

.schedule-item .place .place-arrow-box {
  width: 86rpx;
  height: 54rpx;
  text-align: center;
  line-height: 54rpx;
  float: right;
  border-left: 1px solid #2e9dab;
}

.schedule-item .posi-box {
  margin: 0 1%;
  border-bottom: 6rpx solid #e7e7e7;
  overflow: hidden;
  transition: height 0.4s;
}

.schedule-item .posi-box.border-n {
  border: none;
}

.schedule-item .posi-box.off {
  height: 0!important;
  border-bottom: none!important;
}

.schedule-item .posi-box .posi-item {
  position: relative;
  width: 100%;
  height: 90rpx;
  line-height: 52rpx;
  background-color: #fff;
  padding: 18rpx 0;
}

.schedule-item .posi-box .posi-item view {
  display: inline-block;
  vertical-align: middle;
}

.schedule-item .posi-box .posi-item .border-b {
  position: absolute;
  bottom: 0;
  display: block;
  width: 90%;
  margin: 0 5%;
  border-bottom: 1px solid #eee;
}

.schedule-item .posi-box .posi-item .icon {
  margin: 0 30rpx;
}

.schedule-item .posi-box .posi-item .posi-arrow-box {
  float: right;
  width: 74rpx;
  height: 54rpx;
  text-align: center;
  line-height: 54rpx;
}
